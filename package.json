{
  "name": "html-play",
  "description": "Fetch and parse dynamic HTMLs with Node.js like a boss ðŸ•¶",
  "version": "1.3.0",
  "scripts": {
    "build:types": "tsc",
    "build:esm": "swc src -d dist/esm --strip-leading-paths",
    "build:umd": "swc src -d dist/umd --strip-leading-paths -C module.type=umd",
    "build": "conc npm:build:*",
    "dev": "tsx --test --test-only --no-deprecation",
    "lint": "eslint .",
    "test": "tsx --test --no-deprecation"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "keywords": ["fetch", "crawler"],
  "author": {
    "name": "arianrhodsandlot",
    "email": "theguidanceofawhitetower@gmail.com"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@arianrhodsandlot/eslint-config": "0.8.5",
    "@swc/cli": "0.3.9",
    "@swc/core": "1.4.0",
    "@tsconfig/node-lts": "20.1.1",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.17",
    "@types/serve-handler": "6.1.4",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "playwright": "1.41.2",
    "serve-handler": "6.1.5",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "playwright": "^1.41.1"
  },
  "dependencies": {
    "jsdom": "^24.0.0"
  }
}
